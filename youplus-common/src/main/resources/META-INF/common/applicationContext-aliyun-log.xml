<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
          http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="logClient" class="com.aliyun.openservices.log.Client" >
        <constructor-arg index="0" value="${aliyun.log.endpoint}"/>
        <constructor-arg index="1" value="${aliyun.mq.access_key_id}"/>
        <constructor-arg index="2" value="${aliyun.mq.access_key_secret}"/>
    </bean>
</beans>