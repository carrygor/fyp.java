<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://code.alibabatech.com/schema/dubbo 
		http://code.alibabatech.com/schema/dubbo/dubbo.xsd
		http://www.springframework.org/schema/beans  
		http://www.springframework.org/schema/beans/spring-beans.xsd 
		http://www.springframework.org/schema/context 
	 	http://www.springframework.org/schema/context/spring-context-4.1.xsd">

	<dubbo:service  ref="upCmsUserService"
					  interface="cn.com.youplus.cms.api.auto.UpCmsUserService"
					  version="${dubbo.interface.version}" />

	<dubbo:service  ref="upCmsRequestLogService"
					interface="cn.com.youplus.cms.api.auto.UpCmsRequestLogService"
					version="${dubbo.interface.version}" />

	<dubbo:service  ref="upCmsRolePermissionService"
					interface="cn.com.youplus.cms.api.auto.UpCmsRolePermissionService"
					version="${dubbo.interface.version}" />

	<dubbo:service  ref="upCmsRoleService"
					interface="cn.com.youplus.cms.api.auto.UpCmsRoleService"
					version="${dubbo.interface.version}" />

	<!-- -->
	<dubbo:service  ref="cmsPermissionService"
					interface="cn.com.youplus.cms.api.system.CmsPermissionService"
					version="${dubbo.interface.version}" />
</beans>
